{
  "schemaVersion": 1,
  "id": "fxlib",
  "version": "${version}",
  "name": "FxLib",
  "description": "Assorted utilities for Filloax's mods",
  "authors": ["Filloax"],
  "contributors": [],
  "contact": {},
  "license": "MIT",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "value": "com.filloax.fxlib.FxLibFabric",
        "adapter": "kotlin"
      }
    ]
  },
  "mixins": [
    "fxlib.mixins.json"
  ],
  "depends": {
    "fabric-language-kotlin": ">=1.10.19+kotlin.1.9.23",
    "fabricloader": ">=0.15.11",
    "fabric-api": ">=0.97.8+1.20.6",
    "fabric": "*"
  },
  "accessWidener": "fxlib.accesswidener",
  "custom": {
    "modmenu": {
      "badges": [ "library" ]
    }
  }
}
