{
  "schemaVersion": 1,
  "id": "fxlib",
  "version": "${version}",
  "name": "FXLib",
  "description": "Assorted utilities for Filloax's mods",
  "authors": ["Filloax"],
  "contributors": [],
  "contact": {},
